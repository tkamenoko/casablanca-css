{
  "name": "ladle-example",
  "dependencies": {
    "open-props": "^1.7.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@casablanca-css/core": "^1.0.0",
    "@casablanca-css/styled": "^1.0.1",
    "@ladle/react": "^5.0.2",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@types/react": "~19.0.12",
    "@types/react-dom": "~19.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "postcss-nested": "7.0.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "private": true,
  "scripts": {
    "serve": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "wireit": {
    "serve": {
      "command": "ladle serve",
      "service": true,
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build",
        "../../packages/utils:build"
      ]
    },
    "types": {
      "command": "tsc --noemit",
      "files": [
        "./.ladle/**/*.{ts,tsx}",
        "./src/**/*.{ts,tsx}",
        "./vite.config.ts",
        "./tsconfig.json"
      ],
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build"
      ],
      "output": []
    }
  }
}
