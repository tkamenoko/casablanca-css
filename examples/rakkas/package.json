{
  "name": "rakkas-example",
  "dependencies": {
    "open-props": "^1.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@casablanca-css/core": "^1.0.0",
    "@casablanca-css/styled": "^1.0.1",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@types/react": "~18.2.79",
    "@types/react-dom": "~18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "postcss-nested": "7.0.2",
    "rakkasjs": "0.7.0-next.43",
    "typescript": "^5.3.3",
    "vite": "^6.2.2",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "3.0.9"
  },
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "preview": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "wireit": {
    "dev": {
      "command": "rakkas",
      "service": true,
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build",
        "../../packages/utils:build"
      ]
    },
    "build": {
      "command": "rakkas build",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build",
        "../../packages/utils:build"
      ],
      "files": [
        "./src/",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/"
      ]
    },
    "preview": {
      "command": "rakkas preview",
      "dependencies": [
        "build"
      ],
      "service": true
    },
    "types": {
      "command": "tsc --noemit",
      "files": [
        "./src/**/*.{ts,tsx}"
      ],
      "dependencies": [
        "../../packages/core:types",
        "../../packages/styled:types"
      ],
      "output": []
    }
  }
}
