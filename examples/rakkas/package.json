{
  "name": "rakkas-example",
  "dependencies": {
    "polished": "^4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@macrostyles/core": "0.0.1",
    "@macrostyles/react": "0.0.1",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "rakkasjs": "0.7.0-next.38",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "preview": "wireit"
  },
  "type": "module",
  "wireit": {
    "dev": {
      "command": "rakkas",
      "service": true,
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/react:build"
      ]
    },
    "build": {
      "command": "rakkas build",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/react:build"
      ],
      "files": [
        "./src/",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/"
      ]
    },
    "preview": {
      "command": "rakkas preview",
      "dependencies": [
        "build"
      ],
      "service": true
    }
  }
}
