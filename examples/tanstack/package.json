{
  "name": "tanstack-example",
  "dependencies": {
    "@tanstack/react-router": "1.131.50",
    "@tanstack/react-start": "1.131.50",
    "open-props": "^1.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@casablanca-css/core": "^1.0.0",
    "@casablanca-css/styled": "^1.0.1",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "postcss-nested": "7.0.2",
    "typescript": "^5.3.3",
    "vite": "6.x",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "3.x"
  },
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "preview": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "wireit": {
    "dev": {
      "command": "vite dev",
      "service": true,
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build",
        "../../packages/utils:build",
        "../../packages/eval:build"
      ]
    },
    "build": {
      "command": "vite build",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build",
        "../../packages/utils:build",
        "../../packages/eval:build"
      ],
      "files": [
        "./src/",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/"
      ]
    },
    "preview": {
      "command": "vite preview",
      "dependencies": [
        "build"
      ],
      "service": true
    },
    "types": {
      "command": "tsc --noemit",
      "files": [
        "./src/**/*.{ts,tsx}",
        "./vite.config.ts",
        "./tsconfig.json"
      ],
      "dependencies": [
        "../../packages/core:build",
        "../../packages/styled:build"
      ],
      "output": []
    }
  }
}
