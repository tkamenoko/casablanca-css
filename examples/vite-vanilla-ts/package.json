{
  "name": "vanilla-ts-example",
  "description": "casablanca-css example for vanilla-ts.",
  "version": "0.0.0",
  "dependencies": {
    "open-props": "^1.7.4"
  },
  "devDependencies": {
    "@casablanca-css/core": "^1.0.0",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "postcss-nested": "7.0.2",
    "typescript": "~5.8.2",
    "vite": "^7.1.5"
  },
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "preview": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "wireit": {
    "build": {
      "command": "vite build",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "files": [
        "./src/",
        "./index.html",
        "./vite.config.ts"
      ],
      "dependencies": [
        "../../packages/core:build",
        "../../packages/utils:build"
      ],
      "output": [
        "./dist/**"
      ]
    },
    "dev": {
      "command": "vite",
      "service": true,
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build",
        "../../packages/utils:build"
      ]
    },
    "preview": {
      "command": "vite preview",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "types": {
      "command": "tsc --noemit",
      "files": [
        "./src/**/*.{ts,tsx}",
        "./vite.config.ts",
        "./tsconfig.json"
      ],
      "dependencies": [
        "../../packages/core:build"
      ],
      "output": []
    }
  }
}
