{
  "name": "casablanca-css",
  "description": "Zero-Runtime CSS-in-JS powered by vite.",
  "version": "0.0.1",
  "author": "Tessei Kameyama <tkamenoko@vivaldi.net>",
  "bugs": "https://github.com/tkamenoko/casablanca-css/issues",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@tsconfig/strictest": "^2.0.2",
    "@types/fs-extra": "~11.0.4",
    "@types/prettier": "^3.0.0",
    "fs-extra": "^11.2.0",
    "git-cliff": "^2.4.0",
    "husky": "^9.1.4",
    "postcss-styled-syntax": "^0.7.0",
    "prettier": "^3.2.5",
    "rollup-plugin-node-externals": "^8.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^37.0.0",
    "syncpack": "^13.0.0",
    "typescript": "~5.8.2",
    "typesync": "^0.14.0",
    "vite": "^6.2.2",
    "vite-node": "^3.0.9",
    "vite-plugin-dts": "^4.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "wireit": "^0.14.4"
  },
  "engines": {
    "node": ">=20.15.1"
  },
  "homepage": "https://github.com/tkamenoko/casablanca-css#readme",
  "license": "MIT",
  "private": true,
  "repository": "tkamenoko/casablanca-css.git",
  "scripts": {
    "prepare": "husky || true",
    "set-deps-version": "wireit",
    "test-all": "wireit",
    "types-examples": "wireit",
    "types-packages": "wireit"
  },
  "type": "module",
  "wireit": {
    "test-all": {
      "dependencies": [
        "./packages/core:test",
        "./packages/eval:test",
        "./packages/styled:test"
      ]
    },
    "types-packages": {
      "dependencies": [
        "./packages/core:types",
        "./packages/eval:types",
        "./packages/styled:types",
        "./packages/utils:types"
      ]
    },
    "types-examples": {
      "dependencies": [
        "./examples/ladle:types",
        "./examples/rakkas:types",
        "./examples/vite-react-ts:types",
        "./examples/vite-vanilla-ts:types"
      ]
    },
    "set-deps-version": {
      "command": "vite-node ./scripts/set-deps-verson.ts"
    }
  },
  "workspaces": [
    "./packages/*",
    "./examples/*",
    "./website"
  ]
}
