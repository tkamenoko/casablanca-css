{
  "name": "casablanca-css",
  "description": "Zero-Runtime CSS-in-JS powered by vite.",
  "version": "0.0.1",
  "author": "Tessei Kameyama <tkamenoko@vivaldi.net>",
  "bugs": "https://github.com/tkamenoko/casablanca-css/issues",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@tsconfig/strictest": "^2.0.2",
    "@types/fs-extra": "~11.0.4",
    "@types/prettier": "^3.0.0",
    "fs-extra": "^11.2.0",
    "git-cliff": "^2.4.0",
    "husky": "^9.1.4",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "rollup-plugin-node-externals": "^7.0.1",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "syncpack": "^12.3.0",
    "typescript": "^5.3.3",
    "typesync": "^0.12.1",
    "vite-node": "2.0.3",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "2.0.3",
    "wireit": "^0.14.4"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "homepage": "https://github.com/tkamenoko/casablanca-css#readme",
  "license": "MIT",
  "private": true,
  "repository": "tkamenoko/casablanca-css.git",
  "scripts": {
    "prepare": "husky || true",
    "set-deps-version": "wireit",
    "test-all": "wireit",
    "types-all": "wireit"
  },
  "type": "module",
  "wireit": {
    "test-all": {
      "dependencies": [
        "./packages/core:test",
        "./packages/styled:test"
      ]
    },
    "types-all": {
      "dependencies": [
        "./packages/core:types",
        "./packages/styled:types",
        "./packages/utils:types"
      ]
    },
    "set-deps-version": {
      "command": "vite-node ./scripts/set-deps-verson.ts"
    }
  },
  "workspaces": [
    "./packages/*",
    "./examples/*"
  ]
}
