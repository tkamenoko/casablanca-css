{
  "name": "@macrostyles/core",
  "description": "Zero-Runtime CSS-in-JS powered by vite.",
  "version": "0.0.1",
  "author": "Tessei Kameyama <tkamenoko@vivaldi.net>",
  "bugs": "https://github.com/tkamenoko/macrostyles/issues",
  "dependencies": {
    "@babel/core": "^7.22.20",
    "postcss": "^8.4.29",
    "postcss-nested": "^6.0.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-node-externals": "^6.1.1"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "default": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "homepage": "https://github.com/tkamenoko/macrostyles#readme",
  "license": "MIT",
  "module": "./dist/index.js",
  "peerDependencies": {
    "vite": "^4.4.9"
  },
  "repository": "tkamenoko/macrostyles.git",
  "scripts": {
    "build": "wireit",
    "test": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "./src/**/*.ts",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/**/*.{js,d.ts}"
      ]
    },
    "test": {
      "command": "vitest run",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "files": [
        "./src/**/*.ts",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": []
    },
    "types": {
      "command": "tsc --noEmit",
      "files": [
        "./src/**/*.tsx?"
      ],
      "output": []
    }
  }
}
