{
  "name": "@macrostyles/core",
  "version": "0.0.1",
  "description": "Zero-Runtime CSS-in-JS powered by vite.",
  "type": "module",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "default": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./babel": {
      "default": "./dist/babel.js",
      "types": "./dist/babel.d.ts"
    }
  },
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "./src/**/*.ts",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/**/*.{js,d.ts}"
      ]
    },
    "test": {
      "command": "vitest run",
      "files": [
        "src/**/*.ts"
      ],
      "output": []
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkamenoko/macrostyles.git"
  },
  "author": "Tessei Kameyama <tkamenoko@vivaldi.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkamenoko/macrostyles/issues"
  },
  "homepage": "https://github.com/tkamenoko/macrostyles#readme",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "postcss": "^8.4.21",
    "postcss-nested": "^6.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
