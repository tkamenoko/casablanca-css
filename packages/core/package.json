{
  "name": "@casablanca/core",
  "description": "Zero-Runtime CSS-in-JS powered by vite.",
  "version": "0.0.1",
  "author": "Tessei Kameyama <tkamenoko@vivaldi.net>",
  "bugs": "https://github.com/tkamenoko/casablanca-css/issues",
  "dependencies": {
    "@ampproject/remapping": "^2.3.0",
    "@babel/core": "^7.23.9",
    "@casablanca/utils": "0.0.1",
    "happy-dom": "14.10.1",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "open-props": "^1.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "development": "./src/vite/index.ts",
      "default": "./dist/vite.js"
    }
  },
  "homepage": "https://github.com/tkamenoko/casablanca-css#readme",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^5.0.12"
  },
  "repository": "tkamenoko/casablanca-css.git",
  "scripts": {
    "build": "wireit",
    "lint": "wireit",
    "test": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "../utils:build"
      ],
      "files": [
        "./src/**/*.ts",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/**/*.{js,d.ts}"
      ]
    },
    "lint": {
      "command": "biome check --apply ./",
      "files": [
        "./src/**/*.{ts,tsx}",
        "./vite.config.ts",
        "../../biome.json"
      ],
      "output": []
    },
    "test": {
      "command": "vitest run",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "files": [
        "./src/**/*.{ts,tsx}",
        "./tsconfig.json",
        "./vite.config.ts",
        "../utils/src/**/*.{ts,tsx}",
        "../utils/tsconfig.json",
        "../utils/vite.config.ts"
      ],
      "output": []
    },
    "types": {
      "command": "tsc --noEmit",
      "files": [
        "./src/**/*.{ts,tsx}",
        "./tsconfig.json",
        "./vite.config.ts",
        "../utils/src/**/*.{ts,tsx}",
        "../utils/tsconfig.json",
        "../utils/vite.config.ts"
      ],
      "output": []
    }
  }
}
