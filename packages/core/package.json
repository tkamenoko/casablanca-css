{
  "author": "Tessei Kameyama <tkamenoko@vivaldi.net>",
  "bugs": {
    "url": "https://github.com/tkamenoko/macrostyles/issues"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "postcss": "^8.4.23",
    "postcss-nested": "^6.0.1"
  },
  "description": "Zero-Runtime CSS-in-JS powered by vite.",
  "devDependencies": {
    "@types/babel__core": "^7.20.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./babel": {
      "default": "./dist/babel.js",
      "types": "./dist/babel.d.ts"
    },
    "./vite": {
      "default": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "homepage": "https://github.com/tkamenoko/macrostyles#readme",
  "license": "MIT",
  "module": "./dist/index.js",
  "name": "@macrostyles/core",
  "peerDependencies": {
    "vite": "^4.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkamenoko/macrostyles.git"
  },
  "scripts": {
    "build": "wireit",
    "test": "wireit",
    "types": "wireit"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "version": "0.0.1",
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "./src/**/*.ts",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/**/*.{js,d.ts}"
      ]
    },
    "test": {
      "command": "vitest run",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "files": [
        "./src/**/*.ts",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": []
    },
    "types": {
      "command": "tsc --noEmit",
      "files": [
        "./src/**/*.tsx?"
      ],
      "output": []
    }
  }
}
