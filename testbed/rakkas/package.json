{
  "name": "test-rakkas",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@macrostyles/core": "npm:core@0.0.1",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "rakkasjs": "0.6.20",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "start": "wireit"
  },
  "type": "module",
  "wireit": {
    "dev": {
      "command": "rakkas",
      "service": true,
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build"
      ]
    },
    "build": {
      "command": "rakkas build",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "dependencies": [
        "../../packages/core:build"
      ],
      "files": [
        "./src/",
        "./tsconfig.json",
        "./vite.config.ts"
      ],
      "output": [
        "./dist/"
      ]
    },
    "start": {
      "command": "node ./dist/server",
      "dependencies": [
        "build"
      ],
      "service": true
    }
  }
}
